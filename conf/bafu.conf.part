source src_ch_bafu_alpweiden-herdenschutzhunde : def_searchable_features
{
    sql_db = bafu_dev
    sql_query = \
        SELECT bgdi_id as id \
            , 'feature' as origin \
            , name as detail \
            , 'ch.bafu.alpweiden-herdenschutzhunde' as layer \
            , quadindex(the_geom) as geom_quadindex \
            , st_y(st_transform(st_centroid(the_geom),4326)) as lat \
            , st_x(st_transform(st_centroid(the_geom),4326)) as lon \
            , box2d(the_geom) as geom_st_box2d \
            , name::text as label \
            , bgdi_id::text as feature_id \
        from fauna.alpenweiden_herdenschutzhunde
}


